@{
}
@model IEnumerable<string>

<a asp-controller="Home" asp-action="Index" asp-route-bookCategory="" asp-route-pageNum ="1"class="btn btn-block btn-secondary">Home</a>
<br />

@foreach (string c in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-bookCategory="@c" asp-route-pageNum ="1" class="btn btn-block
       @(c == ViewBag.SelectedType
           ? "btn-primary": "btn-outline-secondary")">@c</a>
    
}